		telnet.c $F.c \
	-lwiringPi -lasound -lm -lfftw3 -lfftw3f -pthread -lncurses \
	`pkg-config --cflags gtk+-3.0` `pkg-config --libs gtk+-3.0`
echo "<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<"
